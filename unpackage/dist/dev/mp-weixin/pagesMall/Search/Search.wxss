.Search {
  padding: 20rpx 4.27% 0;
}
.Search .Search_input {
  margin-bottom: 40rpx;
  width: 100%;
  height: 80rpx;
  background: #FFFFFF;
  box-shadow: 0px 16rpx 40rpx 0px rgba(210, 210, 210, 0.5);
  border-radius: 16rpx;
  position: relative;
}
.Search .Search_input input {
  padding-left: 60rpx;
  padding-right: 40rpx;
  width: calc(100% - 100rpx);
  height: 100%;
  background: #FFFFFF;
  border-radius: 16rpx;
}
.Search .Search_input image {
  position: absolute;
  top: 28rpx;
  left: 24rpx;
  width: 24rpx;
  height: 24rpx;
}
.Search .Search_content {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.Search .Search_content .Search_content_box {
  margin-bottom: 40rpx;
  width: 320rpx;
}
.Search .Search_content .Search_content_box .Search_content_box_img {
  width: 320rpx;
  height: 320rpx;
  padding: 14rpx;
  background: #F6F6F6;
}
.Search .Search_content .Search_content_box .Search_content_box_img image {
  max-width: 100%;
  max-height: 100%;
}
.Search .Search_content .Search_content_box .Search_content_box_price {
  margin: 12rpx 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.Search .Search_content .Search_content_box .Search_content_box_price text {
  font-size: 32rpx;
  font-family: OPPOSans-B, OPPOSans;
  font-weight: normal;
  color: #FB861E;
  line-height: 50rpx;
}
.Search .Search_content .Search_content_box .Search_content_box_price image {
  width: 32rpx;
  height: 30rpx;
}
.Search .Search_content .Search_content_box .Search_content_box_title {
  font-size: 28rpx;
  font-weight: 400;
  color: #24252A;
  line-height: 40rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}
.Search .Search_content .Search_content_box .Search_content_box_shoppingCart {
  margin-top: 12rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.Search .Search_content .Search_content_box .Search_content_box_shoppingCart image {
  width: 34rpx;
  height: 32rpx;
}
.Search .Search_content .Search_content_box .Search_content_box_shoppingCart text {
  margin-left: 8rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #9FA0A2;
  line-height: 40rpx;
  text-decoration: underline;
}
.Search .Search_null {
  padding-top: 200rpx;
  text-align: center;
}
.Search .Search_null image {
  width: 282rpx;
  height: 220rpx;
}
.Search .Search_null text {
  display: block;
  margin-top: 90rpx;
  font-size: 30rpx;
  font-weight: 400;
  color: #24252A;
  line-height: 42rpx;
}

