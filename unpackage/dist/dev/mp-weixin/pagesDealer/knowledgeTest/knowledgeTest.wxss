.knowledgeTeseStyle {
  width: 686rpx;
  height: 280rpx;
  margin: 16rpx auto;
  background-color: rgba(238, 238, 238, 0.5);
  border-radius: 15rpx;
  text-align: center;
  position: relative;
}
.knowledgeTeseStyle .knowledgeTeseStyleBacImg {
  position: absolute;
  left: 0;
  top: 0;
  width: 686rpx;
  height: 280rpx;
  opacity: 0.2;
}
.knowledgeTeseStyle .knowledgeTeseStyleBacImg image {
  width: 100%;
  height: 100%;
}
.knowledgeTeseStyle .knowledgeTeseStyleTitle {
  padding-top: 45rpx;
  font-size: 38rpx;
  font-weight: bold;
  line-height: 60rpx;
  display: block;
}
.knowledgeTeseStyle .knowledgeTeseStyleNumber {
  font-size: 38rpx;
  font-weight: bold;
  line-height: 60rpx;
  margin: 0 5rpx;
}
.knowledgeTeseStyle .knowledgeTeseStyleContent {
  font-size: 26rpx;
  line-height: 50rpx;
  display: block;
}
.knowledgeTeseStyle .knowledgeTeseStyleButton {
  margin-top: 15rpx;
  width: 216rpx;
  height: 58rpx;
  color: #FFFFFF;
  line-height: 58rpx;
  font-size: 28rpx;
  border-radius: 58rpx;
}

