.consultingFeedback {
  padding: 20rpx 4.27% 0;
}
.consultingFeedback .consultingFeedback_textarea {
  padding: 24rpx 32rpx;
  width: 100%;
  height: 560rpx;
  background: #ffffff;
  box-shadow: 0px 20rpx 202rpx -126rpx rgba(19, 31, 43, 0.67);
  border: 2rpx solid rgba(130, 130, 130, 0.33);
}
.consultingFeedback .consultingFeedback_title {
  display: block;
  text-align: center;
  margin-top: 8rpx;
  margin-bottom: 64rpx;
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #828282;
  line-height: 48rpx;
}
.consultingFeedback .consultingFeedback_button {
  width: 320rpx;
  height: 88rpx;
  background: #68b74d;
  border-radius: 54rpx;
  font-size: 30rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: bold;
  color: #ffffff;
  line-height: 88rpx;
}

