<view><block wx:for="{{quizList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="quizStyle"><view class="quizStyleTopic"><text><text style="margin-right:20rpx;">{{index<9?'0'+(index+1):index+1}}</text>{{item.question}}</text></view><radio-group id="{{index}}" data-event-opts="{{[['change',[['radioChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{item.answer_content}}" wx:for-item="items" wx:for-index="indexs" wx:key="*this"><label class="quizLabelStyle"><view class="quizRadioStyle"><radio color="#88c075" value="{{items}}" checked="{{items==item.answer}}"></radio></view><block wx:if="{{items==item.answer}}"><view class="quizContentStyles">{{items}}</view></block><block wx:else><view class="quizContentStyle">{{items}}</view></block></label></block></radio-group></view></block><button class="quizStyleButton" type="primary" data-event-opts="{{[['tap',[['submit']]]]}}" bindtap="__e">我答完了</button></view>