<view class="quiz data-v-5eb634fc"><view class="quiz_top data-v-5eb634fc"><view class="quiz_top_num data-v-5eb634fc"><text class="quiz_top_num_left data-v-5eb634fc">{{num<10?'0'+num:num}}</text><text class="quiz_top_num_right data-v-5eb634fc">{{"/ "+quizList.length}}</text></view><text class="quiz_top_text data-v-5eb634fc">{{''+quizList[num-1].question+''}}</text></view><view class="quiz_bottom data-v-5eb634fc"><block wx:for="{{quizList[num-1].answer_content}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['selectClick',[index]]]]]}}" class="{{['quiz_bottom_button data-v-5eb634fc',activeList[num-1]===index?'quiz_bottom_button_active':'']}}" bindtap="__e"><block wx:if="{{index==0}}"><text class="data-v-5eb634fc">[A]</text></block><block wx:if="{{index==1}}"><text class="data-v-5eb634fc">[B]</text></block><block wx:if="{{index==2}}"><text class="data-v-5eb634fc">[C]</text></block><block wx:if="{{index==3}}"><text class="data-v-5eb634fc">[D]</text></block>{{''+item+''}}</view></block><view class="quiz_bottom_switch data-v-5eb634fc"><view data-event-opts="{{[['tap',[['PreviousQuestion']]]]}}" bindtap="__e" class="data-v-5eb634fc"><image src="../static/image/left.png" mode class="data-v-5eb634fc"></image><text class="data-v-5eb634fc">上一题</text></view><block wx:if="{{num<quizList.length}}"><view data-event-opts="{{[['tap',[['NextQuestion']]]]}}" bindtap="__e" class="data-v-5eb634fc"><text class="data-v-5eb634fc">下一题</text><image src="../static/image/right.png" mode class="data-v-5eb634fc"></image></view></block><block wx:else><view data-event-opts="{{[['tap',[['submit']]]]}}" bindtap="__e" class="data-v-5eb634fc"><text class="data-v-5eb634fc">我答完了</text></view></block></view></view></view>